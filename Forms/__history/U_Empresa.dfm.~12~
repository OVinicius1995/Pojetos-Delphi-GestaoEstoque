  inherited fdqQueryPadrao: TFDQuery
    UpdateOptions.AssignedValues = [uvFetchGeneratorsPoint, uvGeneratorName]
    UpdateOptions.FetchGeneratorsPoint = gpImmediate
    UpdateOptions.GeneratorName = 'GEN_ID_EMPRESA'
    UpdateOptions.AutoIncFields = 'ID_EMPRESA'
    SQL.Strings = (
      'select '
      'sa.ID_EMPRESA,'
      'sa.RAZAO_SOCIAL,'
      'sa.N_FANTASIA,'
      'sa.ENDERECO,'
      'sa.NUMERO,'
      'sa.BAIRRO,'
      'sa.CIDADE,'
      'sa.UF,'
      'sa.CEP,'
      'sa.TELEFONE,'
      'sa.CPF_CNPJ,'
      'sa.EMAIL,'
      'sa.LOGO,'
      'sa.CADASTRO'
      ''
      'from empresa sa')
    Left = 664
    Top = 392
